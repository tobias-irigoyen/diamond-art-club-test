            "custom_liquid": "<div class=\"characteristics-container\">\n{% if product.metafields.custom.icon_shape %}\n<img src=\"{{ product.metafields.custom.icon_shape | img_url: '28x28' }}\" alt=\"Top Notes\" width=\"28\" height=\"28\" \/>\n{% endif %}\n<span class=\"characteristics-metafield-name\">Shape<\/span>\n<span> {{ product.metafields.custom.spec_shape }} <\/span>\n<\/div>"
          }
        },
